{
    "name": "laravel/laravel",
    "description": "The Laravel Framework.",
    "keywords": [
        "framework",
        "laravel"
    ],
    "license": "MIT",
    "type": "project",
    "require": {
        "php": ">=7.3",
        "appstract/laravel-opcache": "^3.0",
        "arcanedev/log-viewer": "^5.0",
        "bandwidth-throttle/token-bucket": "^2.0",
        "bepsvpt/secure-headers": "^5.3",
        "christofferok/laravel-emojione": "^4.0",
        "doctrine/dbal": "^2.9",
        "elhebert/laravel-sri": "^2.0",
        "emojione/assets": "^4.5",
        "fideloper/proxy": "^4.0",
        "gstt/laravel-achievements": "^1.0",
        "guzzlehttp/guzzle": "6.*",
        "hootlex/laravel-moderation": "^1.0",
        "intervention/image": "^2.4",
        "kyslik/column-sortable": "^6.0",
        "laravel/framework": "^6.0",
        "laravel/tinker": "^1.0",
        "marcreichel/igdb-laravel": "^0.10.0",
        "mews/purifier": "^3.0",
        "nelexa/zip": "^3.1",
        "paragonie/constant_time_encoding": "^2.0",
        "predis/predis": "^1.1",
        "spatie/laravel-backup": "^6.4",
        "spatie/laravel-cookie-consent": "^2.9",
        "spatie/laravel-image-optimizer": "^1.5",
        "spatie/ssl-certificate": "^1.12",
        "symfony/dom-crawler": "^2.7|^3.0",
        "theodorejb/polycast": "^1.0",
        "voku/anti-xss": "^4.1"
    },
    "require-dev": {
        "facade/ignition": "^1.4",
        "fzaninotto/faker": "^1.4",
        "jasonmccreary/laravel-test-assertions": "^0.3.0",
        "matt-allan/laravel-code-style": "^0.4.0",
        "mockery/mockery": "^1.0",
        "mpociot/laravel-test-factory-helper": "^1.3",
        "nunomaduro/collision": "^3.0",
        "php-coveralls/php-coveralls": "^2.2",
        "phpunit/phpunit": "^8.0",
        "rector/rector": "^0.6.14"
    },
    "config": {
        "preferred-install": "dist",
        "sort-packages": true,
        "optimize-autoloader": true,
        "platform": {
            "ext-mcrypt": "1.0"
        }
    },
    "extra": {
        "laravel": {
            "dont-discover": []
        }
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/"
        },
        "classmap": [
            "database/seeds",
            "database/factories"
        ],
        "files": [
            "app/Helpers/Helpers.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "scripts": {
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover --ansi"
        ],
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate --ansi"
        ]
    }
}
